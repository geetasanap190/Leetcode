//pattern printing 

// *****
// ****
// ***
// **
// *

public class Main {
    public static void main(String[] args) {
int i, j, row=6;   
//outer loop for rows  
for(i=0; i<row; i++)   
{   
//inner loop for columns  
for(j= 0; i+j < row; j++)   
{   
//prints stars   
System.out.print("* ");   
}   
    System.out.println();   
}
//thr
    }
}
